public void selectEnergy() {
  _view.clearData();
  if(initialCondition.equals("Mode 1")) {
    Theta1=0.131;
    Theta2=0.187;
    Omega1=0.358;
    Omega2=0.509;
    E0=E=0.781;
    dt=0.1;
    computePoincareScale();
  } else if(initialCondition.equals("Mode 2")) {
    Theta1=0.201;
    Theta2=-0.281;
    Omega1=-0.0520;
    Omega2=0.0717;
    E0=E=0.781;
    dt=0.1;
    computePoincareScale();
  } else if(initialCondition.equals("High energy")) {
    Theta1=-0.353;
    Theta2=2.189;
    Omega1=34.28;
    Omega2=120.0;
    E0=E=5000.0;
    dt=0.0025;
    computePoincareScale();
  }else{
    energyAction();
  }
}
]]></Code>
