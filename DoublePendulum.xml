public void poincarePanelAction() {
  double x =_view.poincarePlottingPanel.getMouseX();
  double y =_view.poincarePlottingPanel.getMouseY();
  if(!checkValues(x,y,E0)) return;
  if(t!=0){
    colorIndex++; // change the color only if data has already been collected.
  }
  E=E0;  // remove energy drift
  Theta1=x;
  Omega1=y;
  Theta2=0;
  Omega2=-Omega1*Math.cos(Theta1)+Math.sqrt(2*E-4*g-2*Omega1*Omega1+4*g*Math.cos(Theta1)+Omega1*Omega1*Math.cos(Theta1)*Math.cos(Theta1));
  t=0;
  E0=E=.5*2*Omega1*Omega1+.5*Omega2*Omega2+Omega1*Omega2*Math.cos(Theta2-Theta1)+2*g*(1-Math.cos(Theta1))+g*(1-Math.cos(Theta2));
  clearTrails();
  _view.poincareTrail.addPoint(Theta1,Omega1);
  _view.dataRaster.append(colorIndex,Theta1,Omega1);
}

public boolean checkValues(double Theta1, double Omega1, double E){
  return 2*E-4*g-2*Omega1*Omega1+4*g*Math.cos(Theta1)+Omega1*Omega1*Math.cos(Theta1)*Math.cos(Theta1)>=0;
}


]]></Code>
