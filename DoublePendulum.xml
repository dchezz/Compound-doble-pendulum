public void energyAction () {
  Theta2=0;
  Theta1=0;
  Omega1=0;
  Omega2=Math.sqrt(2*E);
  t=0;
  _view.clearData();
  computePoincareScale () ;
}

public void dragAction(){
  _view.clearData();
  t=0;
  E=.5*2*Omega1*Omega1+.5*Omega2*Omega2+Omega1*Omega2*Math.cos(Theta2-Theta1)+2*g*(1-Math.cos(Theta1))+g*(1-Math.cos(Theta2));//KE+PE+29.43;//Total Energy
  computePoincareScale();
}

public void computePoincareScale() {
  theta1max=Math.acos((-E+g*L1*(m1+m2))/(g*(m1+m2)));
  omega1max=Math.sqrt(2*E);
  double omegaStart=0;
  if(E>4*g) {
    theta1max=Math.PI;
    omegaStart=Math.sqrt(2*E-8*g);
  }
  _view.upperBoundaryTrail.clear();
  _view.lowerBoundaryTrail.clear();
  int n=200;
  double theta=-theta1max, dtheta=2*theta1max/(n+1);
  double omega=omegaStart;
  for(int i=0; i<n; i++){
    _view.upperBoundaryTrail.addPoint(theta,omega);
    _view.lowerBoundaryTrail.addPoint(theta,-omega);
    theta+=dtheta;
    double cos=Math.cos(theta);
    omega=Math.sqrt((2*E+4*g*(cos-1))/(2-cos*cos));
  }
  _view.upperBoundaryTrail.addPoint(theta1max,omegaStart);
  _view.lowerBoundaryTrail.addPoint(theta1max,-omegaStart);
}



]]></Code>
