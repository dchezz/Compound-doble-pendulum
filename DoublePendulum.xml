//This calculates alpha 1 and alpha 2

double mew=m2/(m1+m2);
double lambda=L1/L2;
double term1=1/(1-mew*Math.cos(Theta2-Theta1)*Math.cos(Theta2-Theta1));

double alpha1=term1*(mew*g/L1*Math.sin(Theta2)*Math.cos(Theta2-Theta1)+mew*Omega1*Omega1*Math.sin(Theta2-Theta1)*Math.cos(Theta2-Theta1)-g/L1*Math.sin(Theta1)+mew/lambda*Omega2*Omega2*Math.sin(Theta2-Theta1));

double alpha2=term1*(g/L2*Math.sin(Theta1)*Math.cos(Theta2-Theta1)-mew*Omega2*Omega2*Math.sin(Theta2-Theta1)*Math.cos(Theta2-Theta1)-g/L2*Math.sin(Theta2)-lambda*Omega1*Omega1*Math.sin(Theta2-Theta1));

//alternative expressions not used:(-g*(2*m1+m2)*Math.sin(Theta1)-m2*g*Math.sin(Theta1-2*Theta2)-2*Math.sin(Theta1-Theta2)*m2*(Omega2*Omega2*L2+Omega1*Omega1*L1*Math.cos(Theta1-Theta2)))/(L1*(2*m1+m2-m2*Math.cos(2*Theta1-2*Theta2)));
//alternative expressions not used:(2*Math.sin(Theta1-Theta2)*(Omega1*Omega1*L1*(m1+m2)+g*(m1+m2)*Math.cos(Theta1)+Omega2*Omega2*L2*m2*Math.cos(Theta1-Theta2)))/(L2*(2*m1+m2-m2*Math.cos(2*Theta1-2*Theta2)));
